{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  name: 'AboutUs',\n  data() {\n    return {\n      activeFilter: \"all\",\n      // Filtre actif\n      filters: [{\n        label: \"Tous\",\n        value: \"all\"\n      }, {\n        label: \"Design\",\n        value: \"design\"\n      }, {\n        label: \"Développement\",\n        value: \"dev\"\n      }, {\n        label: \"Montage Vidéo\",\n        value: \"video\"\n      }],\n      items: [{\n        id: 1,\n        name: \"Photoshop\",\n        category: \"design\"\n      }, {\n        id: 2,\n        name: \"Figma\",\n        category: \"design\"\n      }, {\n        id: 3,\n        name: \"React JS\",\n        category: \"dev\"\n      }, {\n        id: 4,\n        name: \"Angular\",\n        category: \"dev\"\n      }, {\n        id: 5,\n        name: \"Adobe Premiere Pro\",\n        category: \"video\"\n      }, {\n        id: 6,\n        name: \"Blender\",\n        category: \"video\"\n      }]\n    };\n  },\n  computed: {\n    filteredItems() {\n      if (this.activeFilter === \"all\") {\n        return this.items;\n      }\n      return this.items.filter(item => item.category === this.activeFilter);\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeFilter","filters","label","value","items","id","category","computed","filteredItems","filter","item"],"sources":["C:\\Users\\REDI\\Desktop\\Doc-Pr\\school\\src\\AboutUs.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-6 bg-gray-100 min-h-screen\">\r\n    <!-- Barre de navigation -->\r\n    <div class=\"flex gap-4 mb-6\">\r\n      <button\r\n        v-for=\"filter in filters\"\r\n        :key=\"filter.value\"\r\n        @click=\"activeFilter = filter.value\"\r\n        :class=\"[\r\n          'px-4 py-2 rounded text-white',\r\n          activeFilter === filter.value ? 'bg-blue-500' : 'bg-gray-400 hover:bg-gray-500'\r\n        ]\"\r\n      >\r\n        {{ filter.label }}\r\n      </button>\r\n    </div>\r\n\r\n    <!-- Zone principale des fichiers -->\r\n    <div class=\"grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4\">\r\n      <div\r\n        v-for=\"item in filteredItems\"\r\n        :key=\"item.id\"\r\n        class=\"p-4 bg-white border border-gray-300 rounded shadow hover:scale-105 transform transition\"\r\n      >\r\n        {{ item.name }}\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name:'AboutUs',\r\n  data() {\r\n    return {\r\n      activeFilter: \"all\", // Filtre actif\r\n      filters: [\r\n        { label: \"Tous\", value: \"all\" },\r\n        { label: \"Design\", value: \"design\" },\r\n        { label: \"Développement\", value: \"dev\" },\r\n        { label: \"Montage Vidéo\", value: \"video\" },\r\n      ],\r\n      items: [\r\n        { id: 1, name: \"Photoshop\", category: \"design\" },\r\n        { id: 2, name: \"Figma\", category: \"design\" },\r\n        { id: 3, name: \"React JS\", category: \"dev\" },\r\n        { id: 4, name: \"Angular\", category: \"dev\" },\r\n        { id: 5, name: \"Adobe Premiere Pro\", category: \"video\" },\r\n        { id: 6, name: \"Blender\", category: \"video\" },\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    filteredItems() {\r\n      if (this.activeFilter === \"all\") {\r\n        return this.items;\r\n      }\r\n      return this.items.filter((item) => item.category === this.activeFilter);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Aucun style personnalisé requis, Tailwind CSS gère tout */\r\n</style>\r\n"],"mappings":";;AA+BA,eAAe;EACbA,IAAI,EAAC,SAAS;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MAAE;MACrBC,OAAO,EAAE,CACP;QAAEC,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAM,CAAC,EAC/B;QAAED,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAS,CAAC,EACpC;QAAED,KAAK,EAAE,eAAe;QAAEC,KAAK,EAAE;MAAM,CAAC,EACxC;QAAED,KAAK,EAAE,eAAe;QAAEC,KAAK,EAAE;MAAQ,CAAC,CAC3C;MACDC,KAAK,EAAE,