oncat(pages.slice(-4));\r\n        } else {\r\n          return [1, '...', this.currentPage - 1, this.currentPage, this.currentPage + 1, '...', this.totalPages];\r\n        }\r\n      }\r\n\r\n      return pages;\r\n    },\r\n  },methods: {\r\n  changePage(page) {\r\n    if (page !== '...') {\r\n      this.currentPage = page;\r\n    }\r\n  },\r\n  prevPage() {\r\n    if (this.currentPage > 1) {\r\n      this.currentPage -= 1;\r\n    }\r\n  },\r\n  nextPage() {\r\n    if (this.currentPage < this.totalPages) {\r\n      this.currentPage += 1;\r\n    }\r\n  },\r\n  ellipsisClick(position) {\r\n    if (position === 'start') {\r\n      // Aller directement à la première page intermédiaire après la page 1\r\n      this.currentPage = 3;\r\n    } else if (position === 'end') {\r\n      // Aller directement à la première page intermédiaire avant la dernière page\r\n      this.currentPage = this.totalPages - 2;\r\n    }\r\n  },\r\n},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.page-item {\r\n  cursor: pointer;\r\n  padding: 0.5rem 1rem;\r\n}\r\n</style>\r\n"],"mappings":";AAqEA,OAAOA,MAAK,MAAO,8BAA6B;AAEhD,eAAe;EACZC,IAAI,EAAE,aAAa;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEH,IAAI,EAAE,QAAQ;QAAEI,KAAK,EAAEL,MAAM;QAAEM,KAAK,EAAC,SAAS;QAAEC,WAAW,EAAC;MAA8F,CAAC,CAErK;MACDC,WAAW,EAAE,CAAC;MAAE;MAChBC,YAAY,EAAE,EAAE,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACV,KAAK,CAACW,MAAK,GAAI,IAAI,CAACL,YAAY,CAAC;IACzD,CAAC;IACDM,cAAcA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACR,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;MACxD,MAAMQ,GAAE,GAAID,KAAI,GAAI,IAAI,CAACP,YAAY;MACrC,OAAO,IAAI,CAACN,KAAK,CAACe,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IACrC,CAAC;IACDE,eAAeA,CAAA,EAAG;MAChB,MAAMC,KAAI,GAAI,EAAE;MAEhB,IAAI,IAAI,CAACT,UAAS,IAAK,CAAC,EAAE;QACxB;QACA,KAAK,IAAIU,CAAA,GAAI,CAAC,EAAEA,CAAA,IAAK,IAAI,CAACV,UAAU,EAAEU,CAAC,EAAE,EAAED,KAAK,CAACE,IAAI,CAACD,CAAC,CAAC;MAC1D,OAAO,IAAI,IAAI,CAACb,WAAU,IAAK,CAAC,EAAE