e déclencher l'animation\r\n      menuOpen: false, // Etat du menu hamburger\r\n      Nav,\r\n      Barer\r\n    }\r\n  },props:{\r\n  nameButton: {type:String, required:true},\r\n  redirect:{type:String, required:true}\r\n },\r\n  mounted() {\r\n    // Déclenche l'animation après le montage du composant\r\n    setTimeout(() => {\r\n      this.hasLoaded = true;\r\n    }, 1000); // Légère pause pour l'effet\r\n  },\r\n  methods: {\r\n  toggleMenu() {\r\n    console.log(\"Menu Open before toggle:\", this.menuOpen); // Vérification de la valeur avant le changement\r\n    this.menuOpen = !this.menuOpen;\r\n    console.log(\"Menu Open after toggle:\", this.menuOpen); // Vérification de la valeur après le changement\r\n  }\r\n}\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"],"mappings":"AAgHA,OAAOA,UAAS,MAAO,wBAAwB;AAC/C,OAAOC,YAAW,MAAO,2CAA2C;AACpE,OAAOC,GAAE,MAAO,2BAA0B;AAC1C,OAAOC,KAAI,MAAO,4BAA2B;AAE7C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLL,UAAU;MACVM,SAAS,EAAE,KAAK;MAChBL,YAAY;MACZM,SAAS,EAAE,KAAK;MAAE;MAClBC,QAAQ,EAAE,KAAK;MAAE;MACjBN,GAAG;MACHC;IACF;EACF,CAAC;EAACM,KAAK,EAAC;IACRC,UAAU,EAAE;MAACC,IAAI,EAACC,MAAM;MAAEC,QAAQ,EAAC;IAAI,CAAC;IACxCC,QAAQ,EAAC;MAACH,IAAI,EAACC,MAAM;MAAEC,QAAQ,EAAC;IAAI;EACrC,CAAC;EACAE,OAAOA,CAAA,EAAG;IACR;IACAC,UAAU,CAAC,MAAM;MACf,IAAI,CAACT,SAAQ,GAAI,IAAI;IACvB,CAAC,EAAE,IAAI,CAAC,EAAE;EACZ,CAAC;EACDU,OAAO,EAAE;IACTC,UAAUA,CAAA,EAAG;MACXC,OAAO,CAACC,GAAG,CAAC,0BAA0B,EAAE,IAAI,CAACZ,QAAQ,CAAC,EAAE;MACxD,IAAI,CAACA,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9BW,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAE,IAAI,CAACZ,QAAQ,CAAC,EAAE;IACzD;EACF;AACA,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}