{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      items: Array.from({\n        length: 50\n      }, (_, i) => `Élément ${i + 1}`),\n      // Exemple de données\n      currentPage: 1,\n      // Page de démarrage\n      itemsPerPage: 5 // Nombre d'éléments par page\n    };\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.items.length / this.itemsPerPage);\n    },\n    paginatedItems() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.items.slice(start, end);\n    },\n    paginationPages() {\n      const pages = [];\n      if (this.totalPages <= 5) {\n        // Afficher tous les boutons si totalPages est inférieur ou égal à 5\n        for (let i = 1; i <= this.totalPages; i++) pages.push(i);\n      } else if (this.currentPage <= 3) {\n        // Afficher les 3 premiers boutons, \"...\" et le dernier\n        pages.push(1, 2, 3, '...', this.totalPages);\n      } else if (this.currentPage >= this.totalPages - 2) {\n        // Afficher le premier bouton, \"...\" et les 3 derniers boutons\n        pages.push(1, '...', this.totalPages - 2, this.totalPages - 1, this.totalPages);\n      } else {\n        // Afficher le premier bouton, \"...\" , page courante +/-1, \"...\" et le dernier\n        pages.push(1, '...', this.currentPage - 1, this.currentPage, this.currentPage + 1, '...', this.totalPages);\n      }\n      return pages;\n    }\n  },\n  methods: {\n    changePage(page) {\n      if (page !== '...') {\n        this.currentPage = page;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage -= 1;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage += 1;\n      }\n    },\n    ellipsisClick(position) {\n      if (position === '...') {\n        // Avance ou recule en fonction de l'emplacement de \"...\"\n     