{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/navigation\";\nimport SwiperCore from \"swiper\";\nimport CourseDetaille from \"../src/components/ComponenrsDetail/CourseDetaille.vue\";\nSwiperCore.use([]);\nexport default {\n  name: \"CourseComponent\",\n  components: {\n    Swiper,\n    SwiperSlide,\n    CourseDetaille\n  },\n  data() {\n    return {\n      activeFilter: \"programming\",\n      currentSlide: 0,\n      // Index du slide actif\n      filters: [{\n        label: \"Programmation\",\n        value: \"programming\",\n        width: \"115px\"\n      }, {\n        label: \"Marketing\",\n        value: \"marketing\",\n        width: \"85px\"\n      }, {\n        label: \"Graphisme\",\n        value: \"design\",\n        width: \"83px\"\n      }],\n      items: [{\n        id: 1,\n        name: \"React JS\",\n        title: \"Devenez un expert du développement front-end\",\n        description: \"Apprenez à maîtriser React.js, la bibliothèque JavaScript incontournable...\",\n        image: \"../src/assets/LogoCours/React.png\",\n        category: \"programming\"\n      }\n      // ... autres cours\n      ]\n    };\n  },\n  computed: {\n    filteredItems() {\n      return this.items.filter(item => item.category === this.activeFilter);\n    }\n  },\n  methods: {\n    updateSlide() {\n      this.currentSlide = this.$refs.swiper.swiper.realIndex;\n    },\n    goToSlide(index) {\n      this.$refs.swiper.swiper.slideToLoop(index);\n      this.currentSlide = index;\n    }\n  }\n};","map":{"version":3,"names":["Swiper","SwiperSlide","SwiperCore","CourseDetaille","use","name","components","data","activeFilter","currentSlide","filters","label","value","width","items","id","title","description","image","category","computed","filteredItems","filter","item","methods","updateSlide","$refs","