\n          @slideChange=\"updateSlide\"\r\n          :breakpoints=\"{\r\n            640: { slidesPerView: 1 },\r\n            1024: { slidesPerView: 2 },\r\n            1280: { slidesPerView: 3 }\r\n          }\"\r\n          class=\"mySwiper\"\r\n        >\r\n          <swiper-slide\r\n            v-for=\"item in filteredItems\"\r\n            :key=\"item.id\"\r\n          >\r\n            <CourseDetaille\r\n              :ImgCours=\"item.image\"\r\n              :NameCours=\"item.name\"\r\n              :TitleCours=\"item.title\"\r\n              :Description=\"item.description\"\r\n            />\r\n          </swiper-slide>\r\n        </swiper>\r\n\r\n        <!-- Indicateurs -->\r\n        <div class=\"absolute bottom-4 left-1/2 transform -translate-x-1/2 flex space-x-2 z-10\">\r\n          <button\r\n            v-for=\"(slide, index) in filteredItems\"\r\n            :key=\"index\"\r\n            :class=\"[\r\n              'w-3 h-3 rounded-full',\r\n              currentSlide === index ? 'bg-blue-500' : 'bg-gray-300'\r\n            ]\"\r\n            @click=\"goToSlide(index)\"\r\n          ></button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\r\nimport \"swiper/css\";\r\nimport \"swiper/css/navigation\";\r\nimport SwiperCore from \"swiper\";\r\nimport CourseDetaille from \"../src/components/ComponenrsDetail/CourseDetaille.vue\";\r\n\r\nSwiperCore.use([]);\r\n\r\nexport default {\r\n  name: \"CourseComponent\",\r\n  components: {\r\n    Swiper,\r\n    SwiperSlide,\r\n    CourseDetaille,\r\n  },\r\n  data() {\r\n    return {\r\n      activeFilter: \"programming\",\r\n      currentSlide: 0, // Index du slide actif\r\n      filters: [\r\n        { label: \"Programmation\", value: \"programming\", width: \"115px\" },\r\n        { label: \"Marketing\", value: \"marketing\", width: \"85px\" },\r\n        { label: \"Graphisme\", value: \"design\", width: \"83px\" },\r\n      ],\r\n      items: [\r\n        {\r\n          id: 1,\r\n          name: \"React JS\",\r\n          title: \"Devenez un expert du développement front-end\",\r\n          description: \"Apprenez à maîtriser React.js, la bibliothèque JavaScript incontournable...\",\r\n          image: \"../src/assets/LogoCours/React.png\",\r\n          category: \"programming\",\r\n        },\r\n        // ... autres cours\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    filteredItems() {\r\n      return this.items.filter((item) => item.category === this.activeFilter);\r\n    },\r\n  },\r\n  methods: {\r\n    updateSlide() {\r\n      this.currentSlide = this.$refs.swiper.swiper.realIndex;\r\n    },\r\n    goToSlide(index) {\r\n      this.$refs.swiper.swiper.slideToLoop(index);\r\n      this.currentSlide = index;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Styles pour personnaliser les indicateurs */\r\nbutton {\r\n  transition: background-color 0.3s ease;\r\n}\r\n</style>\r\n"],"mappings":";;AAuFA,SAASA,MAAM,EAAEC,WAAU,QAAS,YAAY;AAChD,OAAO,YAAY;AACnB,OAAO,uBAAuB;AAC9B,OAAOC,UAAS,MAAO,QAAQ;AAC/B,OAAOC,cAAa,MAAO,uDAAuD;AAElFD,UAAU,CAACE,GAAG,CAAC,EAAE,CAAC;AAElB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVN,MAAM;IACNC,WAAW;IACXE;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,aAAa;MAC3BC,YAAY,EAAE,CAAC;MAAE;MACjBC,OAAO,EAAE,CACP;QAAEC,KAAK,EAAE,eAAe;QAAEC,KAAK,EAAE,aAAa;QAAEC,KAAK,EAAE;MAAQ,CAAC,EAChE;QAAEF,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAO,CAAC,EACzD;QAAEF,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAO,CAAC,CACvD;MACDC,KAAK,EAAE,CACL;QACEC,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,UAAU;QAChBW,KAAK,EAAE,8CAA8C;QACrDC,WAAW,EAAE,6EAA6E;QAC1FC,KAAK,EAAE,mCAAmC;QAC1CC,QAAQ,EAAE;MACZ;MACA;MAAA;IAEJ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACP,KAAK,CAACQ,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACJ,QAAO,KAAM,IAAI,CAACX,YAAY,CAAC;IACzE;EACF,CAAC;EACDgB,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAAChB,YAAW,GA