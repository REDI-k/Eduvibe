ods: {\r\n    nextSlide() {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide = (this.currentSlide + 1) % this.slides.length;\r\n    },\r\n    prevSlide() {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide =\r\n        (this.currentSlide - 1 + this.slides.length) % this.slides.length;\r\n    },\r\n    goToSlide(index) {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide = index;\r\n    },\r\n    startSwipe(event) {\r\n      this.isSwiping = true;\r\n      this.startX = event.touches ? event.touches[0].clientX : event.clientX;\r\n    },\r\n    handleSwipe(event) {\r\n      if (!this.isSwiping) return;\r\n      const currentX = event.touches ? event.touches[0].clientX : event.clientX;\r\n      const diffX = this.startX - currentX;\r\n\r\n      if (diffX > 50) {\r\n        this.isSwiping = false;\r\n        this.nextSlide();\r\n      } else if (diffX < -50) {\r\n        this.isSwiping = false;\r\n        this.prevSlide();\r\n      }\r\n    },\r\n    endSwipe() {\r\n      this.isSwiping = false;\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n\r\n"],"mappings":"AAsEA,OAAOA,MAAK,MAAO,sCAAqC;AACxD,OAAOC,IAAG,MAAO,mCAAkC;AACnD,OAAOC,GAAE,MAAO,uCAAsC;AACtD,OAAOC,QAAO,MAAO,iDAAiD;AAEtE,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAC;IACXF;EACA,CAAC;EACDG,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,MAAM,EAAC,CACL;QACEC,IAAI,EAACR,MAAM;QACXS,KAAK,EAAC,sBAAsB;QAC5BC,WAAW,EAAC;MACd,CAAC,EACD;QACEF,IAAI,EAACP,IAAI;QACTQ,KAAK,EAAC,qBAAqB;QAC3BC,WAAW,EAAC;MACd,CAAC,EACD;QACEF,IAAI,EAACN,GAAG;QACRO,KAAK,EAAC,iBAAiB;QACvBC,WAAW,EAAC;MACd,CAAC;IAEL;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACC,aAAY,GAAI,IAAI,CAACC,YAAY;MACtC,IAAI,CAACA,YAAW,GAAI,CAAC,IAAI,CAACA,YAAW,GAAI,CAAC,IAAI,IAAI,CAACP,MAAM,CAACQ,MAAM;IAClE,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAI,CAACH,aAAY,GAAI,IAAI,CAACC,YAAY;MACtC,IAAI,CAACA,YAAW,GACd,CAAC,IAAI,CAACA,YAAW,GAAI,IAAI,IAAI,CAACP,MAAM,CAACQ,MAAM,IAAI,IAAI,CAACR,MAAM,CAACQ,MAAM;IACrE,CAAC;IACDE,