{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport CourseDetaille from \"../src/components/ComponenrsDetail/CourseDetaille.vue\";\nimport react from \"../src/assets/LogoCours/React.png\";\n// import Angular from \"../src/assets/LogoCours/Angular.png\";\n// import NodeJs from \"../src/assets/LogoCours/NodeJs.png\";\n// import Linux from \"../src/assets/LogoCours/Linux.png\";\n// import Cisco from \"../src/assets/LogoCours/Cisco.png\";\n// import KaliLinux from \"../src/assets/LogoCours/KaliLinux.png\";\n// import Blender from \"../src/assets/LogoCours/Blender.png\";\n// import Poulpe from \"../src/assets/LogoCours/poulpe.png\";\n// import Tareau from \"../src/assets/LogoCours/Tareau.png\";\n// import Cinema4D from \"../src/assets/LogoCours/Cinema4D.png\";\nimport Illustrator from \"../src/assets/LogoCours/Illustrator.png\";\nexport default {\n  name: \"AboutUs\",\n  components: {\n    CourseDetaille\n  },\n  data() {\n    return {\n      activeFilter: \"programming\",\n      filters: [{\n        label: \"Programmation\",\n        value: \"programming\",\n        width: \"115px\"\n      }, {\n        label: \"Marketing\",\n        value: \"marketing\",\n        width: \"85px\"\n      }, {\n        label: \"Graphisme\",\n        value: \"design\",\n        width: \"83px\"\n      }],\n      slides: [{\n        id: 1,\n        name: \"React JS\",\n        title: \"Devenez un expert du développement front-end\",\n        description: \"Apprenez à maîtriser React.js, la bibliothèque JavaScript incontournable pour créer des...\",\n        image: react,\n        category: \"programming\"\n      },\n      // ... (Ajoutez les autres objets slide ici)\n      {\n        id: 11,\n        name: \"Adobe Illustrator\",\n        title: \"Maîtrisez l'essentiel d'Adobe Illustrator\",\n        description: \"Réalisez des illustrations vectorielles, des logos et des graphismes de qualité professionnelle.\",\n        image: Illustrator,\n        category: \"design\"\n      }],\n      currentSlide: 0,\n      previousSlide: null,\n      startX: 0,\n      isSwiping: false\n    };\n  },\n  computed: {\n    filteredItems() {\n      return this.slides.filter(slide => this.activeFilter === \"all\" ? true : slide.category === this.activeFilter);\n    }\n  },\n  methods: {\n    nextSlide() {\n      this.previousSlide = this.currentSlide;\n      this.currentSlide = (this.currentSlide + 1) % this.filteredItems.length;\n    },\n    prevSlide() {\n      this.previousSlide = this.currentSlide;\n      this.currentSlide = (this.currentSlide - 1 + this.filteredItems.length) % this.filteredItems.length;\n    },\n    goToSlide(index) {\n      this.previousSlide = this.currentSlide;\n      this.currentSlide = index;\n    },\n    startSwipe(event) {\n      this.isSwiping = true;\n      this.startX = event.touches ? event.touches[0].clientX : event.clientX;\n    },\n    handleSwipe(event) {\n      if (!this.isSwiping) return;\n      const currentX = event.touches ? event.touches[0].clientX : event.clientX;\n      const diffX = this.startX - currentX;\n      if (diffX > 50) {\n        this.isSwiping = false;\n        this.nextSlide();\n      } else if (diffX < -50) {\n        this.isSwiping = false;\n        this.prevSlide();\n      }\n    },\n    endSwipe() {\n      this.isSwiping = false;\n    }\n  }\n};","map":{"version":3,"names":["CourseDetaille","react","Illustrator","name","components","data","activeFilter","filters","label","value","width","slides","id","title","description","image","category","currentSlide","previousSlide","startX","isSwiping","computed","filteredItems","filter","slide","methods","nextSlide","length","prevSlide","goToSlide","index","startSwipe","event","touches","clientX","handleSwipe","currentX","diffX","endSwipe"],"sources":["C:\\Users\\REDI\\Desktop\\Doc-Pr\\school\\src\\AboutUs.vue"],"sourcesContent":["<template>\r\n  <div class=\"flex justify-center w-full bg-b-main min-h-screen z-10\">\r\n    <div class=\"w-full mb-52 rounded-2xl z-35\">\r\n     <div class=\"flex justify-center\">\r\n      <p class=\"text-white  font-bold text-4xl text-center \">Nos Formations</p>\r\n     </div> \r\n     <div class=\"w-full\">\r\n       <div class=\"mt-12   pb-4 border-b-2 border-cr z-10 ml-1 mr-1 lg:ml-12 lg:mr-12  h-11\">\r\n         <div class=\"flex relative \">\r\n            <div v-for=\"filter in filters\" :key=\"filter.value\" class=\"cursor-pointer  w-52\" \r\n              @click=\"activeFilter = filter.value\">\r\n              <div class=\"flex justify-start\">\r\n                <p :class=\"[ 'font-semibold', \r\n                  activeFilter === filter.value ? 'text-by ' : 'text-white  hover:text-by'\r\n                    ]\" style=\"margin-left:1px; margin-right:4px \">\r\n                     {{ filter.label }}\r\n                </p>\r\n              </div>\r\n              <div class=\"w-full h-5\">\r\n                <div :class=\"['h-1 rounded-t-10 z-20 ',activeFilter === filter.value ? 'bg-by mr-1  mt-4' : 'mr-1']\"\r\n                  :style=\"{ width: filter.width }\"></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n     </div>\r\n   <div class=\"w-full lg:flex lg:justify-center flex justify-center\">\r\n      <div\r\n        :class=\"[\r\n          'mt-20  lg:flex lg:gap-10 lg:w-[98%]',\r\n          filteredItems.length < 3 ? 'lg:justify-center' : 'lg:justify-center'\r\n        ]\"> \r\n        <CourseDetaille\r\n          v-for=\"item in filteredItems\"\r\n          :key=\"item.id\"\r\n          :ImgCours=\"item.image\"\r\n          :NameCours=\"item.name\"\r\n          :TitleCours=\"item.title\"\r\n          :Description=\"item.description\"/>\r\n      </div>\r\n   </div>\r\n   \r\n    </div>\r\n  </div>\r\n</template>\r\n   \r\n\r\n<script>\r\nimport CourseDetaille from \"../src/components/ComponenrsDetail/CourseDetaille.vue\";\r\n\r\nimport react from \"../src/assets/LogoCours/React.png\";\r\n// import Angular from \"../src/assets/LogoCours/Angular.png\";\r\n// import NodeJs from \"../src/assets/LogoCours/NodeJs.png\";\r\n// import Linux from \"../src/assets/LogoCours/Linux.png\";\r\n// import Cisco from \"../src/assets/LogoCours/Cisco.png\";\r\n// import KaliLinux from \"../src/assets/LogoCours/KaliLinux.png\";\r\n// import Blender from \"../src/assets/LogoCours/Blender.png\";\r\n// import Poulpe from \"../src/assets/LogoCours/poulpe.png\";\r\n// import Tareau from \"../src/assets/LogoCours/Tareau.png\";\r\n// import Cinema4D from \"../src/assets/LogoCours/Cinema4D.png\";\r\nimport Illustrator from \"../src/assets/LogoCours/Illustrator.png\";\r\n\r\nexport default {\r\n  name: \"AboutUs\",\r\n  components: {\r\n    CourseDetaille,\r\n  },\r\n  data() {\r\n    return {\r\n      activeFilter: \"programming\",\r\n      filters: [\r\n        { label: \"Programmation\", value: \"programming\", width: \"115px\" },\r\n        { label: \"Marketing\", value: \"marketing\", width: \"85px\" },\r\n        { label: \"Graphisme\", value: \"design\", width: \"83px\" },\r\n      ],\r\n      slides: [\r\n        {\r\n          id: 1,\r\n          name: \"React JS\",\r\n          title: \"Devenez un expert du développement front-end\",\r\n          description:\r\n            \"Apprenez à maîtriser React.js, la bibliothèque JavaScript incontournable pour créer des...\",\r\n          image: react,\r\n          category: \"programming\",\r\n        },\r\n        // ... (Ajoutez les autres objets slide ici)\r\n        {\r\n          id: 11,\r\n          name: \"Adobe Illustrator\",\r\n          title: \"Maîtrisez l'essentiel d'Adobe Illustrator\",\r\n          description:\r\n            \"Réalisez des illustrations vectorielles, des logos et des graphismes de qualité professionnelle.\",\r\n          image: Illustrator,\r\n          category: \"design\",\r\n        },\r\n      ],\r\n      currentSlide: 0,\r\n      previousSlide: null,\r\n      startX: 0,\r\n      isSwiping: false,\r\n    };\r\n  },\r\n  computed: {\r\n    filteredItems() {\r\n      return this.slides.filter((slide) =>\r\n        this.activeFilter === \"all\" ? true : slide.category === this.activeFilter\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    nextSlide() {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide = (this.currentSlide + 1) % this.filteredItems.length;\r\n    },\r\n    prevSlide() {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide =\r\n        (this.currentSlide - 1 + this.filteredItems.length) %\r\n        this.filteredItems.length;\r\n    },\r\n    goToSlide(index) {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide = index;\r\n    },\r\n    startSwipe(event) {\r\n      this.isSwiping = true;\r\n      this.startX = event.touches ? event.touches[0].clientX : event.clientX;\r\n    },\r\n    handleSwipe(event) {\r\n      if (!this.isSwiping) return;\r\n      const currentX = event.touches ? event.touches[0].clientX : event.clientX;\r\n      const diffX = this.startX - currentX;\r\n\r\n      if (diffX > 50) {\r\n        this.isSwiping = false;\r\n        this.nextSlide();\r\n      } else if (diffX < -50) {\r\n        this.isSwiping = false;\r\n        this.prevSlide();\r\n      }\r\n    },\r\n    endSwipe() {\r\n      this.isSwiping = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";;AAgDA,OAAOA,cAAa,MAAO,uDAAuD;AAElF,OAAOC,KAAI,MAAO,mCAAmC;AACrD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,OAAOC,WAAU,MAAO,yCAAyC;AAEjE,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,aAAa;MAC3BC,OAAO,EAAE,CACP;QAAEC,KAAK,EAAE,eAAe;QAAEC,KAAK,EAAE,aAAa;QAAEC,KAAK,EAAE;MAAQ,CAAC,EAChE;QAAEF,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAO,CAAC,EACzD;QAAEF,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAO,CAAC,CACvD;MACDC,MAAM,EAAE,CACN;QACEC,EAAE,EAAE,CAAC;QACLT,IAAI,