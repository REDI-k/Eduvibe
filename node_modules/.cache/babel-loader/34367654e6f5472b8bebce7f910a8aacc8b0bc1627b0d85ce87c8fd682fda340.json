{"ast":null,"code":"import T1 from '../src/assets/Temoignage/1.jpg';\nimport T2 from '../src/assets/Temoignage/2.jpg';\nimport T3 from '../src/assets/Temoignage/3.jpg';\nimport T4 from '../src/assets/Temoignage/4.jpg';\nimport TestimonialsDetail from './components/ComponenrsDetail/TestimonialsDetail.vue';\nexport default {\n  name: 'TestimonialsComponents',\n  components: {\n    TestimonialsDetail\n  },\n  data() {\n    return {\n      T1: T1,\n      T2: T2,\n      T3: T3,\n      T4: T4\n    };\n  },\n  methods: {\n    nextSlide() {\n      this.previousSlide = this.currentSlide;\n      this.currentSlide = (this.currentSlide + 1) % this.slides.length;\n    },\n    prevSlide() {\n      this.previousSlide = this.currentSlide;\n      this.currentSlide = (this.currentSlide - 1 + this.slides.length) % this.slides.length;\n    },\n    goToSlide(index) {\n      this.previousSlide = this.currentSlide;\n      this.currentSlide = index;\n    },\n    startSwipe(event) {\n      this.isSwiping = true;\n      this.startX = event.touches ? event.touches[0].clientX : event.clientX;\n    },\n    handleSwipe(event) {\n      if (!this.isSwiping) return;\n      const currentX = event.touches ? event.touches[0].clientX : event.clientX;\n      const diffX = this.startX - currentX;\n      if (diffX > 50) {\n        this.isSwiping = false;\n        this.nextSlide();\n      } else if (diffX < -50) {\n        this.isSwiping = false;\n        this.prevSlide();\n      }\n    },\n    endSwipe() {\n      this.isSwiping = false;\n    }\n  }\n};","map":{"version":3,"names":["T1","T2","T3","T4","TestimonialsDetail","name","components","data","methods","nextSlide","previousSlide","currentSlide","slides","length","prevSlide","goToSlide","index","startSwipe","event","isSwiping","startX","touches","clientX","handleSwipe","currentX","diffX","endSwipe"],"sources":["C:\\Users\\REDI\\Desktop\\Doc-Pr\\school\\src\\AboutUs.vue"],"sourcesContent":["<template>\r\n  <!-- w-full bg-b-main  z-10 relative bottom-32 -->\r\n  <div class=\"w-full  bg-b-main  z-10 relative\">\r\n    <!-- w-full mb-60 h-hr bg-sss flex -->\r\n    <div class=\"w-full  h-hr bg-sss \">\r\n      <div class=\"flex items-center\">\r\n        <h1 class=\"text-white text-6xl font-bold mt-1 ml-5 \">Ce qu'on dit de<br> notre <span class=\"text-by\">expertise</span><br> et de notre<br> <span class=\"text-by\">engagement</span> <br> envers<br> <span class=\"text-by\">l'excellence</span>.</h1> \r\n      </div>\r\n\r\n      <div class=\"relative bottom-14\">\r\n        <div class=\"relative left-36 top-24 flex justify-between w-wr\">\r\n        <TestimonialsDetail :ImageUser=\"T1\" NamePost=\"DG Digital Cedetech\" NameUser=\"Jean larel\" Testimonial=\"Des cours précis et faciles à suivre. J'ai gagné en compétences en un rien de temps !. \r\n            Très satisfait des résultats !\"/>\r\n        <TestimonialsDetail :ImageUser=\"T2\" NamePost=\"Web Développeur\" NameUser=\"Sarah valala\" Testimonial=\"Des formations claires, bien structurées et très enrichissantes. \r\n        Une excellente manière d'apprendre à son rythme !\"/>\r\n      </div>\r\n      <!-- <div class=\"relative left-36 top-32 flex justify-between w-wr\">\r\n        <TestimonialsDetail :ImageUser=\"T3\" NamePost=\"Porte-parole du Chef de l’Etat\" NameUser=\"Esther Kazadita\" Testimonial=\"Une approche pratique et intuitive qui m’a permis d’apprendre rapidement. \r\n        Je recommande vivement !\"/>\r\n         <TestimonialsDetail :ImageUser=\"T4\" NamePost=\"Admin réseau  Dital Core\" NameUser=\"Paul Kola\" Testimonial=\"Excellente expérience d'apprentissage en ligne. Les modules sont bien conçus \r\n         et très efficaces !\"/>\r\n      </div> -->\r\n    </div> \r\n\r\n\r\n\r\n\r\n    \r\n    </div>   \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport T1 from '../src/assets/Temoignage/1.jpg'\r\nimport T2 from '../src/assets/Temoignage/2.jpg'\r\nimport T3 from '../src/assets/Temoignage/3.jpg'\r\nimport T4 from '../src/assets/Temoignage/4.jpg'\r\nimport TestimonialsDetail from './components/ComponenrsDetail/TestimonialsDetail.vue'\r\nexport default {\r\n  name: 'TestimonialsComponents',\r\n  components:{\r\n   TestimonialsDetail\r\n  },\r\n  data(){\r\n    return{\r\n     T1:T1,\r\n     T2:T2,\r\n     T3:T3,\r\n     T4:T4\r\n    }\r\n  },\r\n  methods: {\r\n    nextSlide() {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide = (this.currentSlide + 1) % this.slides.length;\r\n    },\r\n    prevSlide() {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide =\r\n        (this.currentSlide - 1 + this.slides.length) % this.slides.length;\r\n    },\r\n    goToSlide(index) {\r\n      this.previousSlide = this.currentSlide;\r\n      this.currentSlide = index;\r\n    },\r\n    startSwipe(event) {\r\n      this.isSwiping = true;\r\n      this.startX = event.touches ? event.touches[0].clientX : event.clientX;\r\n    },\r\n    handleSwipe(event) {\r\n      if (!this.isSwiping) return;\r\n      const currentX = event.touches ? event.touches[0].clientX : event.clientX;\r\n      const diffX = this.startX - currentX;\r\n\r\n      if (diffX > 50) {\r\n        this.isSwiping = false;\r\n        this.nextSlide();\r\n      } else if (diffX < -50) {\r\n        this.isSwiping = false;\r\n        this.prevSlide();\r\n      }\r\n    },\r\n    endSwipe() {\r\n      this.isSwiping = false;\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n\r\n"],"mappings":"AAiCA,OAAOA,EAAC,MAAO,gCAA+B;AAC9C,OAAOC,EAAC,MAAO,gCAA+B;AAC9C,OAAOC,EAAC,MAAO,gCAA+B;AAC9C,OAAOC,EAAC,MAAO,gCAA+B;AAC9C,OAAOC,kBAAiB,MAAO,sDAAqD;AACpF,eAAe;EACbC,IAAI,EAAE,wBAAwB;EAC9BC,UAAU,EAAC;IACVF;EACD,CAAC;EACDG,IAAIA,CAAA,EAAE;IACJ,OAAM;MACLP,EAAE,EAACA,EAAE;MACLC,EAAE,EAACA,EAAE;MACLC,EAAE,EAACA,EAAE;MACLC,EAAE,EAACA;IACJ;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACC,aAAY,GAAI,IAAI,CAACC,YAAY;MACtC,IAAI,CAACA,YAAW,GAAI,CAAC,IAAI,CAACA,YAAW,GAAI,CAAC,IAAI,IAAI,CAACC,MAAM,CAACC,MAAM;IAClE,CAAC;IACDC,S