{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport CourseDetaille from './components/ComponenrsDetail/CourseDetaille.vue';\nimport react from '../src/assets/LogoCours/React.png';\nimport Angular from '../src/assets/LogoCours/Angular.png';\nimport NodeJs from '../src/assets/LogoCours/NodeJs.png';\n// import Linux from '../src/assets/LogoCours/Linux.png';\n// import Cisco from '../src/assets/LogoCours/Cisco.png';\n// import KaliLinux from '../src/assets/LogoCours/KaliLinux.png';\n// import Blender from '../src/assets/LogoCours/Blender.png';\n// import Poulpe from '../src/assets/LogoCours/poulpe.png';\n// import Tareau from '../src/assets/LogoCours/Tareau.png';\n// import Cinema4D from '../src/assets/LogoCours/Cinema4D.png';\n// import Illustrator from '../src/assets/LogoCours/Illustrator.png';\n\nexport default {\n  name: 'AboutUs',\n  components: {\n    CourseDetaille\n  },\n  data() {\n    return {\n      activeFilter: 'programming',\n      filters: [{\n        label: 'Programmation',\n        value: 'programming',\n        width: '115px'\n      }, {\n        label: 'Graphisme',\n        value: 'design',\n        width: '83px'\n      }, {\n        label: 'Marketing',\n        value: 'marketing',\n        width: '85px'\n      }, {\n        label: 'Réseau',\n        value: 'network',\n        width: '52px'\n      }],\n      items: [{\n        id: 1,\n        name: 'Angular',\n        title: 'Devenez un expert du développement front-end',\n        description: \"Apprenez à maîtriser Angular, la techno robuste pour construire des applications web modernes.\",\n        image: Angular,\n        category: 'programming'\n      }, {\n        id: 2,\n        name: 'React JS',\n        title: 'Devenez un expert du développement front-end',\n        description: \"Apprenez à maîtriser React.js, la bibliothèque JavaScript incontournable pour créer des...\",\n        image: react,\n        category: 'programming'\n      }, {\n        id: 3,\n        name: 'Node JS',\n        title: 'Maîtrisez le développement côté serveur',\n        description: \"Apprenez à utiliser Node.js pour créer des applications back-end performantes et scalables.\",\n        image: NodeJs,\n        category: 'programming'\n      }],\n      currentSlide: 0,\n      startX: 0,\n      isSwiping: false,\n      windowWidth: window.innerWidth\n    };\n  },\n  computed: {\n    filteredItems() {\n      if (!this.activeFilter || !this.items) {\n        console.error('Aucune condition de filtre valide ou pas d\\'éléments disponibles');\n        return [];\n      }\n      const filtered = this.items.filter(item => item.category === this.activeFilter);\n      console.log('Filtered Items:', filtered);\n      return filtered;\n    }\n  },\n  methods: {\n    nextSlide() {\n      this.currentSlide = (this.currentSlide + 1) % this.filteredItems.length;\n      this.$nextTick(() => {\n        // Vous pouvez maintenant effectuer une action en toute sécurité sur le DOM\n        console.log('Après changement de slide');\n      });\n    },\n    prevSlide() {\n      this.currentSlide = (this.currentSlide - 1 + this.filteredItems.length) % this.filteredItems.length;\n      this.$nextTick(() => {\n        // Action après changement de slide\n        console.log('Après changement de slide');\n      });\n    },\n    handleSwipe(event) {\n      if (!this.isSwiping) return;\n      const currentX = event.touches ? event.touches[0].clientX : event.clientX;\n      const diffX = this.startX - currentX;\n      if (diffX > 50) {\n        this.nextSlide();\n        this.isSwiping = false;\n      } else if (diffX < -50) {\n        this.prevSlide();\n        this.isSwiping = false;\n      }\n    }\n  },\n  mounted() {\n    this.resizeHandler = () => {\n      this.windowWidth = window.innerWidth;\n      console.log('Liste des éléments :', this.items);\n    };\n    window.addEventListener('resize', this.resizeHandler);\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.resizeHandler);\n  }\n};","map":{"version":3,"names":["CourseDetaille","react","Angular","NodeJs","name","components","data","activeFilter","filters","label","value","width","items","id","title","description","image","category","currentSlide","startX","isSwiping","windowWidth","window","innerWidth","computed","filteredItems","console","error","filtered","filter","item","log","methods","nextSlide","length","$nextTick","prevSlide","handleSwipe","event","currentX","touches","clientX","diffX","mounted","resizeHandler","addEventListener","beforeUnmount","removeEventListener"],"sources":["C:\\Users\\REDI\\Desktop\\Doc-Pr\\school\\src\\AboutUs.vue"],"sourcesContent":["<template>\r\n  <div class=\"flex justify-center w-full bg-b-main min-h-screen z-10\">\r\n    <div class=\"w-full mb-52 rounded-2xl z-35\">\r\n      <div class=\"flex justify-center\">\r\n        <p class=\"text-white font-bold text-4xl text-center\">Nos Formations</p>\r\n      </div>\r\n      <div class=\"w-full\">\r\n        <div class=\"mt-12 pb-4 border-b-2 border-cr z-10 ml-1 mr-1 lg:ml-12 lg:mr-12 h-11\">\r\n          <div class=\"flex relative\">\r\n            <div\r\n              v-for=\"filter in filters\"\r\n              :key=\"filter.value\"\r\n              class=\"cursor-pointer w-52\"\r\n              @click=\"activeFilter = filter.value\">\r\n              <div class=\"flex justify-center\">\r\n                <p\r\n                  :class=\"[ \r\n                    'font-semibold', \r\n                    activeFilter === filter.value \r\n                      ? 'text-by' \r\n                      : 'text-white hover:text-by' \r\n                  ]\"\r\n                  style=\"margin-left:1px; margin-right:4px\"\r\n                >\r\n                  {{ filter.label }}\r\n                </p>\r\n              </div>\r\n              <div class=\"w-full h-5\">\r\n                <div\r\n                  :class=\"[ \r\n                    'h-1 rounded-t-10 z-20',\r\n                    activeFilter === filter.value ? 'bg-by mr-1 mt-4' : 'mr-1',\r\n                  ]\"\r\n                  :style=\"{ width: filter.width }\"\r\n                ></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Section des cours -->\r\n      <div\r\n        @touchstart=\"startSwipe\"\r\n        @touchmove=\"handleSwipe\"\r\n        @touchend=\"endSwipe\"\r\n        @mousedown=\"startSwipe\"\r\n        @mousemove=\"handleSwipe\"\r\n        @mouseup=\"endSwipe\"\r\n        @mouseleave=\"endSwipe\"\r\n      >\r\n        <!-- Carousel pour petits écrans -->\r\n        <div\r\n          v-if=\"filteredItems.length && windowWidth < 1024\"\r\n          class=\"w-full relative overflow-hidden lg:hidden\"\r\n        >\r\n          <div\r\n            class=\"flex items-center justify-center transition-transform duration-500 ease-in-out\"\r\n            :style=\"{ transform: `translateX(-${currentSlide * 100}%)` }\"\r\n          >\r\n            <div\r\n              v-for=\"item in filteredItems\"\r\n              :key=\"item.id\"\r\n              class=\"w-full flex-shrink-0 flex justify-center mt-10\"\r\n            >\r\n              <div class=\"w-[90%] flex justify-center\">\r\n                <CourseDetaille\r\n                  :ImgCours=\"item.image\"\r\n                  :NameCours=\"item.name\"\r\n                  :TitleCours=\"item.title\"\r\n                  :Description=\"item.description\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Boutons de navigation -->\r\n          <div class=\"flex justify-center mt-4 space-x-2\">\r\n            <button\r\n              v-for=\"(item, index) in filteredItems\"\r\n              :key=\"index\"\r\n              :class=\"[ \r\n                'w-3 h-3 rounded-full', \r\n                index === currentSlide ? 'bg-blue-500' : 'bg-gray-300' \r\n              ]\"\r\n              @click=\"goToSlide(index)\"\r\n            ></button>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Section des cours pour grands écrans -->\r\n        <div v-else class=\"mt-20 lg:flex lg:gap-10 lg:w-[98%] lg:justify-center\">\r\n          <CourseDetaille\r\n            v-for=\"item in filteredItems\"\r\n            :key=\"item.id\"\r\n            :ImgCours=\"item.image\"\r\n            :NameCours=\"item.name\"\r\n            :TitleCours=\"item.title\"\r\n            :Description=\"item.description\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CourseDetaille from './components/ComponenrsDetail/CourseDetaille.vue';\r\n\r\nimport react from '../src/assets/LogoCours/React.png';\r\nimport Angular from '../src/assets/LogoCours/Angular.png';\r\nimport NodeJs from '../src/assets/LogoCours/NodeJs.png';\r\n// import Linux from '../src/assets/LogoCours/Linux.png';\r\n// import Cisco from '../src/assets/LogoCours/Cisco.png';\r\n// import KaliLinux from '../src/assets/LogoCours/KaliLinux.png';\r\n// import Blender from '../src/assets/LogoCours/Blender.png';\r\n// import Poulpe from '../src/assets/LogoCours/poulpe.png';\r\n// import Tareau from '../src/assets/LogoCours/Tareau.png';\r\n// import Cinema4D from '../src/assets/LogoCours/Cinema4D.png';\r\n// import Illustrator from '../src/assets/LogoCours/Illustrator.png';\r\n\r\nexport default {\r\n  name: 'AboutUs',\r\n  components: {\r\n    CourseDetaille,\r\n  },\r\n  data() {\r\n    return {\r\n      activeFilter: 'programming',\r\n      filters: [\r\n        { label: 'Programmation', value: 'programming', width: '115px' },\r\n        { label: 'Graphisme', value: 'design', width: '83px' },\r\n        { label: 'Marketing', value: 'marketing', width: '85px' },\r\n        { label: 'Réseau', value: 'network', width: '52px' },\r\n      ],\r\n      items: [\r\n        {\r\n          id: 1,\r\n          name: 'Angular',\r\n          title: 'Devenez un expert du développement front-end',\r\n          description: \"Apprenez à maîtriser Angular, la techno robuste pour construire des applications web modernes.\",\r\n          image: Angular,\r\n          category: 'programming',\r\n        },\r\n        {\r\n          id: 2,\r\n          name: 'React JS',\r\n          title: 'Devenez un expert du développement front-end',\r\n          description: \"Apprenez à maîtriser React.js, la bibliothèque JavaScript incontournable pour créer des...\",\r\n          image: react,\r\n          category: 'programming',\r\n        },\r\n        {\r\n          id: 3,\r\n          name: 'Node JS',\r\n          title: 'Maîtrisez le développement côté serveur',\r\n          description: \"Apprenez à utiliser Node.js pour créer des applications back-end performantes et scalables.\",\r\n          image: NodeJs,\r\n          category: 'programming',\r\n        },\r\n      ],\r\n      currentSlide: 0,\r\n      startX: 0,\r\n      isSwiping: false,\r\n      windowWidth: window.innerWidth,\r\n    };\r\n  },\r\n  computed: {\r\n    filteredItems() {\r\n      if (!this.activeFilter || !this.items) {\r\n        console.error('Aucune condition de filtre valide ou pas d\\'éléments disponibles');\r\n        return [];\r\n      }\r\n      const filtered = this.items.filter((item) => item.category === this.activeFilter);\r\n      console.log('Filtered Items:', filtered);\r\n      return filtered;\r\n    },\r\n  },\r\n  methods: {\r\n  nextSlide() {\r\n    this.currentSlide = (this.currentSlide + 1) % this.filteredItems.length;\r\n    this.$nextTick(() => {\r\n      // Vous pouvez maintenant effectuer une action en toute sécurité sur le DOM\r\n      console.log('Après changement de slide');\r\n    });\r\n  },\r\n  prevSlide() {\r\n    this.currentSlide = (this.currentSlide - 1 + this.filteredItems.length) % this.filteredItems.length;\r\n    this.$nextTick(() => {\r\n      // Action après changement de slide\r\n      console.log('Après changement de slide');\r\n    });\r\n  },\r\n  handleSwipe(event) {\r\n    if (!this.isSwiping) return;\r\n    const currentX = event.touches ? event.touches[0].clientX : event.clientX;\r\n    const diffX = this.startX - currentX;\r\n\r\n    if (diffX > 50) {\r\n      this.nextSlide();\r\n      this.isSwiping = false;\r\n    } else if (diffX < -50) {\r\n      this.prevSlide();\r\n      this.isSwiping = false;\r\n    }\r\n  },\r\n},\r\n\r\n  mounted() {\r\n    this.resizeHandler = () => {\r\n      this.windowWidth = window.innerWidth;\r\n      console.log('Liste des éléments :', this.items);\r\n    };\r\n    window.addEventListener('resize', this.resizeHandler);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('resize', this.resizeHandler);\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";;AA2GA,OAAOA,cAAa,MAAO,kDAAkD;AAE7E,OAAOC,KAAI,MAAO,mCAAmC;AACrD,OAAOC,OAAM,MAAO,qCAAqC;AACzD,OAAOC,MAAK,MAAO,oCAAoC;AACvD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,aAAa;MAC3BC,OAAO,EAAE,CACP;QAAEC,KAAK,EAAE,eAAe;QAAEC,KAAK,EAAE,aAAa;QAAEC,KAAK,EAAE;MAAQ,CAAC,EAChE;QAAEF,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAO,CAAC,EACtD;QAAEF,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAO,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAO,CAAC,CACrD;MACDC,KAAK,EAAE,CACL;QACEC,EAAE,EAAE,CAAC;QACLT,IAAI,EAAE,SAAS;QACfU,KAAK,EAAE,8CAA8C;QACrDC,WAAW,EAAE,gGAAgG;QAC7GC,KAAK,EAAEd,OAAO;QACde,QAAQ,EAAE;MACZ,CAAC,EACD;QACEJ,EAAE,EAAE,CAAC;QACLT,IAAI,EAAE,UAAU;QAChBU,KAAK,EAAE,8CAA8C;QACrDC,WAAW,EAAE,4FAA4F;QACzGC,KAAK,EAAEf,KAAK;QACZgB,QAAQ,EAAE;MACZ,CAAC,EACD;QACEJ,EAAE,EAAE,CAAC;QACLT,IAAI,EAAE,SAAS;QACfU,KAAK,EAAE,yCAAyC;QAChDC,WAAW,EAAE,6FAA6F;QAC1GC,KAAK,EAAEb,MAAM;QACbc,QAAQ,EAAE;MACZ,CAAC,CACF;MACDC,YAAY,EAAE,CAAC;MACfC,MAAM,EAAE,CAAC;MACTC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAEC,M